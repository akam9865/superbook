
<li class="list-group-item">
  <div>
	  <span>
	      <%= game.attributes.team1.city %>
	      <%= game.attributes.team1.name %>
	  </span>
	
		<button
    type="button"
    id="team-1"
    class="btn btn-default pull-right team <%= this.model.attributes.team1.id %>">
      <%= this.model.renderOdds[0][0] + this.model.renderOdds[0][1] %>
    </button>
	</div>
	<br>
  <div>
		<span>
	      <%= game.attributes.team2.city %>
	      <%= game.attributes.team2.name %>
	  </span>

		<button
    type="button"
    id="team-2"
    class="btn btn-default pull-right team <%= this.model.attributes.team2.id %>">
      <%= this.model.renderOdds[1][0] + this.model.renderOdds[1][1] %>
    </button>
  </div>
	<br>
	
</li>