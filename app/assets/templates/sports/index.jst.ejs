
<ul class="list-group">
  <h3 class="list-group-item">sports</h3>
  
  <% sports.each( function (sport) { %>

    <li
      class="list-group-item">
      <a
        class="sport-link"
        href="#/sports/<%= sport.get('id') %>">
          <%= sport.escape("name") %>
      </a>
    </li>

  <% }) %>
</ul>


